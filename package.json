{
  "name": "thecultt-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "env-cmd -f .dev.env react-scripts start",
    "build": "react-scripts build",
    "build:dev": "env-cmd -f .dev.env react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "prettier": "prettier . --write",
    "prettier:check": "prettier . --check",
    "prepare": "husky"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@sentry/react": "^7.92.0",
    "@tcb-web/create-credit": "^1.0.5",
    "array-chunk-split": "^2.0.15",
    "axios": "^0.27.2",
    "browser-image-compression": "^2.0.2",
    "dayjs": "^1.11.11",
    "heic2any": "^0.0.4",
    "qs": "^6.12.1",
    "react": "^18.1.0",
    "react-animate-height": "^3.1.1",
    "react-countup": "^6.5.3",
    "react-dom": "^18.1.0",
    "react-dots-loader": "^1.1.4",
    "react-helmet": "^6.1.0",
    "react-number-format": "^5.2.2",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-scroll": "^1.8.9",
    "react-slick": "^0.29.0",
    "redux": "^4.2.0",
    "redux-form": "^8.3.8",
    "redux-form-input-masks": "^2.0.2",
    "redux-thunk": "^2.4.1",
    "sass": "^1.69.0",
    "slick-carousel": "^1.8.1",
    "swiper": "^11.0.4",
    "usehooks-ts": "^3.1.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^16.11.38",
    "@types/qs": "^6.9.15",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-helmet": "^6.1.5",
    "@types/react-scroll": "^1.8.3",
    "@types/react-slick": "^0.23.10",
    "@types/redux-form": "^8.3.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/parser": "^5.62.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "typescript": "^4.7.3",
    "typescript-eslint": "^7.10.0"
  }
